ide",animationLoop:false,rtl:rtlVal});}},removethumbnail:function(i,c,g){var e=i.attr("ean");var k=i.attr("value");var h=i.attr("imgpath");var b=i.data("brandcode-subbrandcode");if(b==undefined){b=0;}var j=e+"~"+k+"~"+h+"~"+b;if($.cookie(c)!==undefined){var d=$.cookie(c);if(d.indexOf(j)!==-1){var a=d.split("|");var f=a.indexOf(j);a.splice(f,1);$.cookie(c,a.join("|"),{path:"/",domain:domain,expires:365});}}i.parent("li").remove();Basket.totalcart(COOKIETOPRINT,SAMPLETOORDER);if($.cookie(c)==undefined||$.cookie(c)==""){$("#slider-"+g+" .cart__empty").show();}},reConstructCarousel:function(a,b){$("#slider-"+a+" .list-coupons ul.reset").last().find("li:last").appendTo(b);var c=$("#slider-"+a+" .list-coupons ul.reset").last().find("li");if(c.length==0){$("#slider-"+a+" .list-coupons ul.reset").last().parent().parent().remove();}},productPrint:function(a){Basket.setcookie(a,COOKIETOPRINT);Basket.numberitems(COOKIETOPRINT,"coupon");Basket.totalcart(COOKIETOPRINT,SAMPLETOORDER);Basket.btnStyle(COOKIETOPRINT,"print",1);Basket.baskettotal(COOKIETOPRINT);a.addClass("disable-order");if($(a).attr("id")=="overlay"){window.openLayer($(printlayer).val(),480);}else{document.location.href=$(printlayer).val();}},setaside:function(a,c){var b=(c===".recto")?".verso":".recto";var d=a.find(b).height();if(c===".verso"){a.addClass("offer-details");}else{a.removeClass("offer-details");}$(".content-body-inner").isotope("reLayout",{});a.find(c).css({opacity:0}).animate({opacity:1},"fast");},noprint:function(a){a.addClass("offer-no-print");a.find(".recto").stop().animate({opacity:0.1},"fast");if($("#e-sampling").length>0){$(".no-print").slideDown();$(".button.print-coupon").fadeOut();}if($.browser.msie&&$.browser.version<9&&is_Ie10){a.find(".recto").css({visibility:"hidden"});}a.find(".no-print").css({opacity:1}).fadeIn("fast");}};var esampling={init:function(){$("#multisample .inner-sample ul li").on("click",function(){$this=$(this);var c=$(this).find("input");var d=$this.parents(".inner-sampl