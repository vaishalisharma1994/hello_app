nter){z=q(x,D,D+C.GPSInfoIFDPointer,j,v);for(B in z){switch(B){case"GPSVersionID":z[B]=z[B][0]+"."+z[B][1]+"."+z[B][2]+"."+z[B][3];break;}C[B]=z[B];}}return C;}d.getData=function(w,v){if((w instanceof Image||w instanceof HTMLImageElement)&&!w.complete){return false;}if(!k(w)){h(w,v);}else{if(v){v.call(w);}}return true;};d.getTag=function(v,w){if(!k(v)){return;}return v.exifdata[w];};d.getAllTags=function(x){if(!k(x)){return{};}var v,w=x.exifdata,y={};for(v in w){if(w.hasOwnProperty(v)){y[v]=w[v];}}return y;};d.pretty=function(x){if(!k(x)){return"";}var v,w=x.exifdata,y="";for(v in w){if(w.hasOwnProperty(v)){if(typeof w[v]=="object"){if(w[v] instanceof Number){y+=v+" : "+w[v]+" ["+w[v].numerator+"/"+w[v].denominator+"]\r\n";}else{y+=v+" : ["+w[v].length+" values]\r\n";}}else{y+=v+" : "+w[v]+"\r\n";}}}return y;};d.readFromBinaryFile=function(v){return f(v);};if(typeof define==="function"&&define.amd){define("exif-js",[],function(){return d;});}}.call(this));$.ajaxSetup({cache:false});var Search={hasFocus:true,SearchAjax:function(h,g,a){var d="",b="",c="",e=0;if(this.hasFocus==false){return false;}if(g==""||g.length<3){$(h).hide().html("");}else{if(g.length>2){var f=$("#current-culture").html();$.ajax({type:"GET",contentType:"application/json; charset=utf-8",url:a,data:{q:g,c:f},dataType:"json",success:function(i){if(i.length>0){$.each(i,function(){if(this["Type"]!=""){b+='<li class="event_search" data-action-detail="'+this["Text"]+'" data-search-text="'+this["Text"]+'" data-search-page="'+this["Page"]+'"><a href="#">'+this["Text"]+" <span>"+this["Type"]+"</span></a></li>";e++;}else{c+='<li class="event_search" data-action-detail="'+this["Text"]+'"  data-search-text="'+this["Text"]+'" data-search-page="'+this["Page"]+'"><a href="#">'+this["Text"]+"</a></li>";}});d+='<ul class="uiSelect-list">'+b+c+"</ul>";if(g!=""){$(h).html(d).show();if(b!=""){if(e==1){$(h+" ul.uiSelect-list li:first").addClass("brandseperator");}else{if(e>1){$(h+" .uiSelect-list li").eq(e-1).addClass