<script language="javascript">

document.onkeydown = function()
{


if(window.event && window.event.keyCode == 115) 
        { // Capture and remap F5
	window.event.keyCode = 504;
  	}  	
if(window.event && window.event.keyCode == 116) 
        { // Capture and remap F5
	window.event.keyCode = 505;
  	}
 if(window.event && window.event.keyCode == 117) 
        { // Capture and remap F5
	window.event.keyCode = 506;
  	} 	
 if(window.event && window.event.keyCode == 122) 
        { // Capture and remap F5
	window.event.keyCode = 507;
  	} 
 if(window.event && window.event.keyCode == 121) 
        { // Capture and remap F5
	window.event.keyCode = 508;
  	} 
if(window.event && window.event.keyCode == 504) 
        { 
	//alert("'F4' Key is Disabled");
	return false; 
	}
if(window.event && window.event.keyCode == 505) 
        { 
	//alert("'F5' Key is Disabled");
	return false; 
	}
if(window.event && window.event.keyCode == 506) 
        { 
	//alert("'F6' Key is Disabled");
	return false; 
	}	
if(window.event && window.event.keyCode == 508) 
        { // Capture and remap F10
	//alert("'F10' Key is Disabled");
	return false; 
  	} 	
if(window.event && window.event.keyCode == 507) 
        { 
	//alert("'F11' Key is Disabled");
	return false; 
	}  	
	

	if(window.event)
        {
                key = window.event.keyCode;     //IE
                if(window.event.ctrlKey)
                       { isCtrl = true;}
                else
                       { isCtrl = false;}
        }
        if(isCtrl)
        {
               //alert("'Ctrl' Key is Disabled");
               return false;
        }
}


function load(obj)
{

if(obj.flag.value==0)
   {
   
history.go(1);
}

}


function unload(obj)
{

obj.flag.value=0;

}

blServletFlag=true; 
function doubleclick(form)
 {
 
 	if(blServletFlag==true)
  	{
  		blServletFlag=false; 
 		//location.href=stlink;
 		form.submit();
 	}
 	else
 		{
 		alert("Already submited...");
 		//location.href=stlink;
 		form.submit();
 		}
 }
    



function clickIE4(){
if (event.button==2){
alert("works");
return false;
}
}

this.top.window.document.title = "CBI Net Banking";
