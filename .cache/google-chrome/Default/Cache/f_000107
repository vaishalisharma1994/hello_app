function downloadstatement()
{
document. transaction.HandleID.value="SCHEDULED_STMTOFACC";
document.transaction.submit();
}
function validateForm()
{
	//added another condition for -1 check in mozilla by kashyap
	if(document.transaction.acctype.value.length == 0 || document.transaction.acctype.value == "-1"){
	//ends here by kashyap
		alert("Select an Account");
		document.transaction.acctype.focus();
		return false;
	}
	
	if(document.transaction.request_type.value=="-1")
	{
	alert("Please select the Period basis in Transactions for box");
	document.transaction.request_type.focus();
	return false;
	}
	if(document.transaction.transtype.value.length == 0){
		alert(" Enter Transaction Type");
		document.transaction.transtype.focus();
		return false;
	}
	if(document.transaction.disorder.value.length == 0){
		alert(" Enter Display Order");
		document.transaction.disorder.focus();
		return false;
	}
	if((document.transaction.maxAmount.value.length!=0 && document.transaction.minAmount.value.length==0) ||
	   (document.transaction.maxAmount.value.length==0 && document.transaction.minAmount.value.length!=0) )
	{
	    alert("Please Enter both the Minimum & Maximum Amount for the Range");
	    document.transaction.minAmount.focus();
	    document.transaction.minAmount.select();
	    return false;
	}
	if(document.transaction.minAmount.value.length!=0 && !checkNumber("Minimum Amount",document.transaction.minAmount))
	{
		return false;
	}
	if(document.transaction.maxAmount.value.length!=0 && !checkNumber("Maximum Amount",document.transaction.maxAmount))
	{
		return false;
	}
	if(document.transaction.maxAmount.value.length!=0 && document.transaction.minAmount.value.length!=0 )
	{
		var min = document.transaction.minAmount.value ;
		var max = document.transaction.maxAmount.value ;

		if(parseInt(min) >= parseInt(max))
		{
			alert ("Enter valid Range");
			document.transaction.minAmount.select();
			document.transaction.minAmount.focus();
			return false;
		}
		if(document.transaction.currency.selectedIndex==0)
		{
		    alert("Please Select a Currency");
		    document.transaction.currency.focus();
		    return false;
		}
	}
	if(document.transaction.instNumber.value.length!="" && !checkNumberAlone("Instrument Number",document.transaction.instNumber))
	{
	    document.transaction.instNumber.focus();
	    document.transaction.instNumber.select();
		return false;
	}
	
	if(document.transaction.request_type.value=="2" && (document.transaction.fromdate.value.length==0 || document.transaction.todate.value.length==0))
	{
		alert(" Please Enter the Date range for the Specified Period");
		document.transaction.fromdate.select();
		document.transaction.fromdate.focus();
		return false;
	}
	if(document.transaction.fromdate.value.length!=0) 
	{
	    	startDate = document.transaction.fromdate.value;
                
		if(isDateValid(startDate))
        	{
	       		if(yyyy < 1995 || yyyy > 2020)
	       		{
	    			alert("'From date' is invalid");
	    			transaction.fromdate.focus();
	    			transaction.fromdate.select();
	      			return false;
	       		}
	       		startDate = toDate(yyyy,mm,dd);
	     	}
	     	else
	     	{
		      alert("'From date' is invalid");
		      transaction.fromdate.focus();
	      	      transaction.fromdate.select();
	      	      return false;
	       }
	       
	      currentDate=getCurrentDate();
	      
	      if(startDate > currentDate)
	      {
		   alert("'From date' cannot be greater than current date");
	           transaction.fromdate.focus();
		   transaction.fromdate.select();
		   return false;
	      }
	      if(document.transaction.todate.value.length==0)
		{
		alert ("Enter both 'From' and 'To'  dates");
		transaction.todate.focus();
	    	transaction.todate.select();
	      	return false;
		}
	}
	
	if(document.transaction.todate.value.length!=0)
	{
	startDate = document.transaction.fromdate.value;
	endDate = document.transaction.todate.value;
	currentDate=getCurrentDate();
	
	if(document.transaction.fromdate.value.length==0)
	{
		alert ("Enter both 'From' and 'To'  dates");
		transaction.fromdate.focus();
	    	transaction.fromdate.select();
	      	return false;
	}
	if(isDateValid(startDate))
        	{
	       		if(yyyy < 1995 || yyyy > 2020)
	       		{
	    			alert("'From date' is invalid");
	    			transaction.fromdate.focus();
	    			transaction.fromdate.select();
	      			return false;
	       		}
	       		startDate = toDate(yyyy,mm,dd);
	     	}
	     	else
	     	{
		      alert("'From date' is invalid");
		      transaction.fromdate.focus();
	      	      transaction.fromdate.select();
	      	      return false;
	       }
		if(isDateValid(endDate))
	       {
	      	    if(yyyy < 1995 || yyyy > 2020)
	    	   {
	    		alert("'To date' is  invalid");
	    		transaction.todate.focus();
	    		transaction.todate.select();
	      		return false;
	      	    }
	       	    endDate = toDate(yyyy,mm,dd);
	       }
	      else
	      {
	      	   alert("'To Date' is invalid");
	           transaction.todate.focus();
	           transaction.todate.select();
	           return false;
	      }
	      
	      if(endDate > currentDate)
	      {
	      	  alert("'To date' cannot be greater than current date");
		  transaction.todate.focus();
		  transaction.todate.select();
		  return false;
	      }
	      
 	      if(startDate > endDate)
	      {
		 alert("'From Date' cannot be greater than 'To Date'");
		 transaction.todate.focus();
		 transaction.todate.select();
		 return false;
	      }
}
		
	return true;
}
function checkMonthDiff(date1,date2)
{
	var mm1;
	var yy1;
	var mm2;
	var yy2;
	mm1=date1.substring(3,5)
	mmt1=date1.substring(3,4)
	yy1=date1.substring(6,10)
	mm2=date2.substring(3,5)
	mmt2=date2.substring(3,4)
	yy2=date2.substring(6,10)
	if(mmt1 == "0")
	{
	   mm1=mm1.substring(1,2)
	}
	if(mmt2 == "0")
	{
		mm2=mm2.substring(1,2)
	}
    yy1=parseInt(yy1);
    yy2=parseInt(yy2);
    mm1=parseInt(mm1);
    mm2=parseInt(mm2);
	if(mm1 > 11)
	{
		if(  ((yy2 == yy1) &&  (mm2<=12)) ||
		   ((yy2 == (yy1+1)) && (mm2 <= 1)) )
		{
			return true;
		}
		else
		{
			return false;
		}
	}
	else if(mm1 <= 11)
	{
		if((yy1 == yy2) && (mm2<(mm1+3)) && (mm2<=12)) 
		{
			return true;
		}
		else
		{
			return false;
		}
	}
}

function getCurrentPeriod()
{

var cur = new Date();
var curmonth=cur.getMonth()+1;
var year=cur.getFullYear();
if(curmonth<=3)
{
var diff = 3-curmonth;
curmonth = 12-diff;
year=year-1;
}
else if(curmonth>3)
{
var diff=curmonth-3;
curmonth=diff;
}

if(cur.getDate()<10)
{
day = "0"+cur.getDate();
}else
{
day = cur.getDate();
}

if(curmonth<10)
{
curmonth="0"+curmonth;
}

if(curmonth==2)
{
if(day==30)
{
day=28;
}
if(day==31)
{
day=28;
}
}

if(curmonth==4)
{
if(day==31)
{
day=30;
}
}

if(curmonth==6)
{
if(day==31)
{
day=30;
}
}

if(curmonth==9)
{
if(day==31)
{
day=30;
}
}
if(curmonth==11)
{
if(day==31)
{
day=30;
}
}

var startdate = day+"/"+curmonth+"/"+year;
//alert( "startdate" + startdate);
var curmonth1 = cur.getMonth()+1;

if(curmonth1<10)
{
curmonth1="0"+curmonth1;
}
var enddate=day+"/"+curmonth1+"/"+cur.getFullYear();
//alert( "enddate" + enddate);
document.transaction.fromdate.value=startdate;
document.transaction.todate.value=enddate;

}

function submitform()
 {
	if (validateForm())
	{
	
	//alert(document.transaction.formattype.value);
	//alert("from date : " + document.transaction.fromdate.value);
	//alert("To date: " + document.transaction.todate.value);
	
	    if(document.transaction.formattype.value.length != 0){
		    alert("Format Type should not be selected for display options");
		    document.transaction.formattype.focus();
		    return;
	    }
	    	    if(document.transaction.downloadtype.value.length != 0){
		    alert("Download Type should not be selected for display options");
		    document.transaction.downloadtype.focus();
		    return;
	    }
	    
	  //  alert("request type : "+document.transaction.request_type.value);
	    var option1= document.getElementsByName("radioOption");   
	    
	    if(document.transaction.request_type.value == 2 && option1[1].checked)
	    {
	       if(!checkMonthDiff(document.transaction.fromdate.value,document.transaction.todate.value))
	    {
        alert('Start Date and End Date should be in within a period of 3 months');
		document.transaction.todate.focus();
		document.transaction.todate.select();
		return;
	    }
	}
	//  alert("option1[1].checked" +option1[1].checked);
	    if(document.transaction.request_type.value == 1  && option1[1].checked)
		{
			getCurrentPeriod();
		}

	//alert("from date: " + document.transaction.fromdate.value);
	//alert("To date: " + document.transaction.todate.value);
	


	    defineLabel();
	     	var tb1 = document.getElementById('buttons');
	var tb2 = document.getElementById('aftersubmit');
	tb1.style.visibility = "hidden";
	tb2.style.visibility = "visible";
	    
	    if(document.transaction.dislast.value.length  !=  0)
	    {
       	 	document. transaction.HandleID.value="H_TRANNUM450";
       	 	
		document. transaction.submit();
            }
    	    else
    	    {
    		document.transaction.HandleID.value= "102_TXD";
    		
		document.transaction.submit();
    	     }
    	     
	}
}

function submitform1()
 {
	if(validateForm())
	{
	    if(document.transaction.formattype.value.length == 0){
	    	alert(" Format Type should  be selected ");
		    document.transaction.formattype.focus();
		    return;
	    }
	    if(document.transaction.downloadtype.value.length == 0){
	    	alert(" Download Type should  be selected ");
		    document.transaction.downloadtype.focus();
		    return;
	    }
	    //added by priya on 03 may 2011
	     var option2= document.getElementsByName("radioOption"); 
	    if(document.transaction.request_type.value == 2 && option2[1].checked )
	    {
	      if(!checkMonthDiff(document.transaction.fromdate.value,document.transaction.todate.value))
	    {
        alert('Start Date and End Date should be in within a period of 3 months');
		document.transaction.todate.focus();
		document.transaction.todate.select();
		return ;
	    }
	    }
	  
	   
	    if(document.transaction.request_type.value==1 && option2[1].checked)
		{
			getCurrentPeriod();
		}
		defineLabel();
		blFlag = false;
		if(document.transaction.downloadtype.value == "instant")
		{
			blFlag  = confirm("You have selected INSTANT Download of Statment of Account. Do you want to confirm?");
		}
		else if(document.transaction.downloadtype.value == "later")
		{
			blFlag  = confirm("You have selected SCHEDULE LATER Download of Statment of Account. Do you want to confirm?");
		}
		if(blFlag == true)
		{
		if(document.transaction.downloadtype.value == "instant")
		{
			document.transaction.maxnotxns.value = "500";
		}
		else if(document.transaction.downloadtype.value == "later")
		{
			document.transaction.maxnotxns.value = "2000";
		}
		 	var tb1 = document.getElementById('buttons');
	var tb2 = document.getElementById('aftersubmit');
	tb1.style.visibility = "hidden";
	tb2.style.visibility = "visible";
		
		if(document.transaction.dislast.value.length != 0){
        		document. transaction.HandleID.value= "102_TXDN";
        		document. transaction.submit();
        	}
        	else{
        		document. transaction.HandleID.value="102_TXDD";
        		document. transaction.submit();
        	}
		}
		else
		{
			return;
		}
	}
}
function enableDates()
{

if(document.transaction.request_type.value=="1")
{
document.transaction.fromdate.value="";
document.transaction.todate.value="";


}
else if(document.transaction.request_type.value=="2")
{

}
else if(document.transaction.request_type.value=="-1")
{

}

}

function displaydate()
{
if(document.transaction.request_type.value=="2")
{

window.dateField=document.transaction.fromdate; 
//commented by kashyap on 101208 for mozilla check
	calendar=displayWindowOpen('/jsp/calendar.jsp','cal');
	//calendar=window.open('/jsp/calendar.jsp','cal','WIDTH=250,HEIGHT=310');
//calendar=window.open('/jsp/calendar.jsp','cal','WIDTH=290,HEIGHT=310');
}
}

function displayoption(val)
{
if(val==0)
{

	document.getElementById('row1').style.display='none';
		document.getElementById('row2').style.display='none';
			document.getElementById('row3').style.display='none';
			document.getElementById('row4').style.display='none';
						document.getElementById('row5').style.display='none';
			

}else if(val==1)
{

	document.getElementById('row1').style.display='inline';
		document.getElementById('row2').style.display='inline';
			document.getElementById('row3').style.display='inline';
			document.getElementById('row4').style.display='inline';
			document.getElementById('row5').style.display='inline';
}
}
function defineLabel()
{
//alert(document.transaction.HandleID.value);
	 var labelValue="";
   var option=document.getElementsByName("radioOption");
   //alert("lll : " + option.length);
    
	if(option[0].checked)   
{	
    document.transaction.stmnttype.value="default";   
labelValue= "Post&Date"+"-"+"Value&Date"+"-"+"Branch&Code"+"-"+"Cheque&Number"+"-"+"Account&Description"+"-"+"Debit"+"-"+"Credit"+"-"+"Balance";
   }else if(option[1].checked)
   { 
	      document.transaction.stmnttype.value="customized";
     var selected=document.getElementsByName("chkOption");
var chosenValue="";
   for (var i=0;i<selected.length;i++)
	    {
	   
	  
			if(selected[i].checked)
			{
 chosenValue= chosenValue+"-"+selected[i].value;
		
				//labelString=labelString+"-"+ chosenValue

			}
	    }
  
   labelValue=chosenValue.replace(" ","&");
   
	//labelString = "Post&Date"+"-"+"Value&Date"+"-"+"Branch&Code"+"-"+"Cheque&Number"+"-"+"Description"+"-"+"Debit"+"-"+"Credit"+"-"+"Balance";
   }
   
   
   //alert(labelValue);
   //alert(document.transaction.labelnames.value);
  document.transaction.labelnames.value=labelValue;


}
